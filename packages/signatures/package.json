{
  "name": "@metamask/signatures",
  "version": "0.0.0",
  "description": "Example package containing signature-based utilities",
  "keywords": [
    "MetaMask",
    "Ethereum"
  ],
  "homepage": "https://github.com/MetaMask/metamask-monorepo-template/tree/main/packages/signatures#readme",
  "bugs": {
    "url": "https://github.com/MetaMask/metamask-monorepo-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/metamask-monorepo-template.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "ts-bridge --project tsconfig.build.json --verbose --clean --no-references",
    "changelog:update": "../../scripts/update-changelog.sh @metamask/signatures",
    "changelog:validate": "../../scripts/validate-changelog.sh @metamask/signatures",
    "prepare-manifest:preview": "../../scripts/prepare-preview-manifest.sh",
    "publish:preview": "yarn npm publish --tag preview",
    "since-latest-release": "../../scripts/since-latest-release.sh",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --reporters=jest-silent-reporter",
    "test:clean": "NODE_OPTIONS=--experimental-vm-modules jest --clearCache",
    "test:verbose": "NODE_OPTIONS=--experimental-vm-modules jest --verbose",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch"
  },
  "dependencies": {
    "@metamask/greetings": "^0.0.0"
  },
  "devDependencies": {
    "@metamask/auto-changelog": "^3.1.0",
    "@ts-bridge/cli": "^0.5.1",
    "@types/jest": "^27.4.1",
    "deepmerge": "^4.2.2",
    "jest": "^27.5.1",
    "jest-silent-reporter": "^0.5.0",
    "ts-jest": "^27.1.4",
    "typescript": "~5.2.2"
  },
  "engines": {
    "node": "^18.18 || >=20"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
